# http://EditorConfig.org
root = true

# Default Settings
[*]
charset = utf-8
end_of_lines = lf
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.rb]
indent_size = 2

[*.js,*.json]
indent_size = 2

[*.sh,.bashrc,bash_profile]
indent_size = 2

[*.md]
indent_size = 2

[.editorconfig]
indent_size = 2
